@model EmployeeListViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    @foreach(var employee in Model.Employees)
    {
        <div>
            <p>@employee.FirstName @employee.LastName - @employee.Position</p>
        </div>
    }
</div>
